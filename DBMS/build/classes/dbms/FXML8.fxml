<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXPasswordField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="dbms.FXML8Controller">
   <children>
      <JFXPasswordField fx:id="newpass" layoutX="210.0" layoutY="80.0" promptText="NEW PASSWORD" />
      <JFXPasswordField fx:id="confirm" layoutX="210.0" layoutY="148.0" promptText="CONFIRM PASSWORD" />
      <Button layoutX="184.0" layoutY="245.0" mnemonicParsing="false" onAction="#save" prefHeight="34.0" prefWidth="63.0" style="-fx-background-color: blue;" text="SAVE" textFill="WHITE">
         <font>
            <Font name="SansSerif Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="282.0" layoutY="245.0" mnemonicParsing="false" onAction="#nextclick" prefHeight="34.0" prefWidth="63.0" style="-fx-background-color: blue;" text="NEXT" textFill="WHITE">
         <font>
            <Font name="SansSerif Bold" size="14.0" />
         </font>
      </Button>
      <Label fx:id="lab" layoutX="202.0" layoutY="192.0" prefHeight="25.0" prefWidth="186.0" text="Entered passwords must be same" textFill="RED" />
      <Label fx:id="lab1" layoutX="223.0" layoutY="308.0" prefHeight="34.0" prefWidth="96.0" text="CHANGED">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
